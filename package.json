{
  "name": "foodreau",
  "version": "1.0.0",
  "description": "Data supplier plugin for filling textboxes with food recipes",
  "author": "Max David",
  "repository": "https://github.com/maxdavid/sketch-foodreau",
  "license": "MIT",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "foodreau",
    "manifest": "src/manifest.json",
    "main": "foodreau.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "dependencies": {
    "@skpm/fs": "^0.2.3"
  }
}
