{
  "name": "Foodreau",
  "description": "Data supplier plugin for filling textboxes with food recipes",
  "version" : "1.0.2",
  "author": "Max David",
  "homepage": "https://github.com/maxdavid/sketch-foodreau",
  "identifier": "com.maxdavid.sketch.foodreau-plugin",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "suppliesData": true,
  "commands": [
    {
      "name": "Foodreau",
      "script": "foodreau.js"
    },
    {
      "script": "foodreau.js",
      "handlers": {
        "actions": {
          "Startup": "onStartup",
          "Shutdown": "onShutdown",
          "SupplyRandomTitle": "onSupplyRandomTitle",
          "SupplyRandomSource": "onSupplyRandomSource",
          "SupplyRandomContent": "onSupplyRandomContent",
          "SupplyRandomImage": "onSupplyRandomContent",
          "SearchRecipe" : "onSearchRecipe"
        }
      }
    }
  ],
  "menu": {
    "title": "Foodreau"
  }
}
